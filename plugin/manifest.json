{
  "id": "com.adobe.ps.extension.export",
  "name": "PS Extension Export",
  "version": "1.0.0",
  "main": "index.html",
  "manifestVersion": 4,
  "host": {
    "app": "PS",
    "minVersion": "23.0.0"
  },
  "permissions": {
    "webview": {
      "allow": "yes",
      "domains": ["http://127.0.0.1:2271"]
    },
    "allowCodeGenerationFromStrings": true
  },
  "requiredPermissions": {
    "allowCodeGenerationFromStrings": true,
    "launchProcess": {
      "schemes": ["http", "https"],
      "domains": ["127.0.0.1"]
    },
    "allowUnsafeEval": true,
    "clipboard": "readAndWrite",
    "localFileSystem": "fullAccess"
  },
  "entrypoints": [
    {
      "type": "panel",
      "id": "ps_export_panel",
      "label": {
        "default": "PS Export"
      },
      "minimumSize": { "width": 230, "height": 200 },
      "maximumSize": { "width": 2000, "height": 2000 },
      "preferredDockedSize": { "width": 230, "height": 300 },
      "preferredFloatingSize": { "width": 230, "height": 300 },
      "icons": [
        {
          "width": 23,
          "height": 23,
          "path": "icons/dark-panel.png",
          "scale": [1, 2],
          "theme": ["darkest", "dark", "medium"],
          "species": ["chrome"]
        },
        {
          "width": 23,
          "height": 23,
          "path": "icons/light-panel.png",
          "scale": [1, 2],
          "theme": ["lightest", "light"],
          "species": ["chrome"]
        }
      ]
    }
  ],
  "icons": [
    {
      "width": 48,
      "height": 48,
      "path": "icons/plugin.png",
      "scale": [1, 2],
      "theme": ["darkest", "dark", "medium", "lightest", "light", "all"],
      "species": ["pluginList"]
    }
  ]
}
